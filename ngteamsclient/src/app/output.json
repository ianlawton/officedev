application/vnd.microsoft.card.adaptive: {
  "type": "AdaptiveCard",
  "body": [
    {
      "color": "accent",
      "size": "large",
      "text": "👋 Hi there, I'm Polly",
      "wrap": true,
      "type": "TextBlock"
    },
    {
      "text": "A teammate of yours recently added me to help your team create and manage polls.",
      "wrap": true,
      "type": "TextBlock"
    }
  ],
  "actions": [
    {
      "card": {
        "type": "AdaptiveCard",
        "body": [
          {
            "url": "https://urlp.asm.skype.com/v1/url/content?url=https%3a%2f%2fstatic.polly.ai%2fimg%2fmsteams%2fwelcome-carousel-1.gif",
            "height": "auto",
            "spacing": "medium",
            "type": "Image"
          },
          {
            "text": "To create a poll in this channel, simply activate the Polly compose extension via the (···) menu below the message input box.",
            "wrap": true,
            "type": "TextBlock"
          }
        ],
        "actions": [
          {
            "url": "https://www.polly.ai/help/microsoft-teams",
            "title": "Learn More",
            "type": "Action.OpenUrl"
          }
        ]
      },
      "title": "Create a Poll",
      "type": "Action.ShowCard"
    },
    {
      "card": {
        "type": "AdaptiveCard",
        "body": [
          {
            "url": "https://urlp.asm.skype.com/v1/url/content?url=https%3a%2f%2fstatic.polly.ai%2fimg%2fmsteams%2fwelcome-carousel-2.gif",
            "height": "auto",
            "spacing": "medium",
            "type": "Image"
          },
          {
            "text": "Pin Polly to your compose box to access it from any of your chats or teams.",
            "wrap": true,
            "spacing": "medium",
            "type": "TextBlock"
          }
        ],
        "actions": [
          {
            "url": "https://www.polly.ai/help/microsoft-teams",
            "title": "Learn More",
            "type": "Action.OpenUrl"
          }
        ]
      },
      "title": "Pin Polly",
      "type": "Action.ShowCard"
    },
    {
      "card": {
        "type": "AdaptiveCard",
        "body": [
          {
            "url": "https://urlp.asm.skype.com/v1/url/content?url=https%3a%2f%2fstatic.polly.ai%2fimg%2fmsteams%2fwelcome-carousel-3.gif",
            "height": "auto",
            "spacing": "medium",
            "type": "Image"
          },
          {
            "text": "You can close or delete polls via the \"View My Polls\" section when activating Polly from the channel where you posted the poll.",
            "wrap": true,
            "spacing": "medium",
            "type": "TextBlock"
          }
        ],
        "actions": [
          {
            "url": "https://www.polly.ai/help/microsoft-teams",
            "title": "Learn More",
            "type": "Action.OpenUrl"
          }
        ]
      },
      "title": "Manage Polls",
      "type": "Action.ShowCard"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0"
}
